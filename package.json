{
  "name": "babel-plugin-universal-import",
  "version": "3.1.2",
  "description": "Babel plugin to transform import() into its Universal counterpart",
  "main": "index.js",
  "repository": "github:faceyspacey/babel-plugin-universal-import.git",
  "bugs": "https://github.com/faceyspacey/babel-plugin-universal-import/issues",
  "homepage": "https://github.com/faceyspacey/babel-plugin-universal-import#readme",
  "author": "James Gillmore <james@faceyspacey.com>",
  "contributors": [
    "Zack Jackson <zack@ScriptedAlchemy.com> (https://github.com/ScriptedAlchemy)"
  ],
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "lint": "eslint --fix ./",
    "format": "prettier --single-quote --semi=false --write '{src,__tests__,__test-helpers__}/**/*.js' && npm run lint",
    "precommit": "lint-staged --verbose && npm test",
    "cm": "git-cz",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "babel",
    "universal",
    "react",
    "universal import",
    "redux-first-router",
    "ruc"
  ],
  "files": [
    "*.png",
    "universalImport.js"
  ],
  "dependencies": {
    "@babel/helper-module-imports": "^7.10.4"
  },
  "peerDependencies": {
    "webpack": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.11.4",
    "@babel/plugin-transform-modules-commonjs": "7.10.4",
    "babel-plugin-tester": "9.2.0",
    "commitizen": "4.0.4",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "7.7.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.6",
    "jest": "26.4.2",
    "lint-staged": "10.2.13",
    "prettier": "2.1.1",
    "semantic-release": "17.1.1",
    "travis-github-status": "1.6.3"
  },
  "jest": {
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --semi=false --write",
      "eslint --fix"
    ]
  }
}
