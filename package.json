{
  "name": "babel-plugin-universal-import",
  "version": "3.1.2",
  "description": "Babel plugin to transform import() into its Universal counterpart",
  "main": "index.js",
  "repository": "github:faceyspacey/babel-plugin-universal-import.git",
  "bugs": "https://github.com/faceyspacey/babel-plugin-universal-import/issues",
  "homepage": "https://github.com/faceyspacey/babel-plugin-universal-import#readme",
  "author": "James Gillmore <james@faceyspacey.com>",
  "contributors": [
    "Zack Jackson <zack@ScriptedAlchemy.com> (https://github.com/ScriptedAlchemy)"
  ],
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "lint": "eslint --fix ./",
    "format": "prettier --single-quote --semi=false --write '{src,__tests__,__test-helpers__}/**/*.js' && npm run lint",
    "precommit": "lint-staged && npm test",
    "cm": "git-cz",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "keywords": [
    "babel",
    "universal",
    "react",
    "universal import",
    "redux-first-router",
    "ruc"
  ],
  "files": [
    "*.png",
    "universalImport.js"
  ],
  "dependencies": {
    "@babel/helper-module-imports": "^7.10.4"
  },
  "peerDependencies": {
    "webpack": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.11.4",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-modules-commonjs": "7.10.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "21.2.0",
    "babel-plugin-tester": "5.5.2",
    "commitizen": "4.0.4",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "jest": "21.2.1",
    "lint-staged": "5.0.0",
    "prettier": "1.19.1",
    "semantic-release": "6.3.6",
    "travis-github-status": "1.6.3"
  },
  "jest": {
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "prettier --single-quote --semi=false --write",
        "eslint --fix",
        "git add"
      ]
    },
    "verbose": true
  }
}
